{
  "env": {
    "es6": true
  },
  "extends": [
    "react-app",
    "prettier"
  ],
  "rules": {
    "no-var": "error",
    "semi": "warn",
    "prefer-const": "warn",
    "no-use-before-define": "error",
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "react-hooks/exhaustive-deps": "warn"
  },
  "plugins": [
    "prettier",
    "react",
    "react-hooks"
  ]
}
